      -
        code : |1-
         // If the name is not empty
         if EDT_LastName = "" then
         	Close()
         end
         
         // Creation or renaming mode? 
         if bCreation = True then
         	if fMakeDir(gsInProgress+[fSep]+EDT_LastName) = True then
         		ToastDisplay("Directory created successfully")
         		
         		// Reset the display window
         		if WIN_DisplayFolder.State = Active then
         			WIN_DisplayFolder.WindowInitialisation(gsInProgress+[fSep]+EDT_LastName)
         		end
         		
         	end
         else
         	if fRename(gsInProgress,fExtractPath(gsInProgress,fDrive+fDirectory)+[fSep]+EDT_LastName) = True then
         		ToastDisplay("Directory renamed successfully")
         	end
         end
         
         Close()
