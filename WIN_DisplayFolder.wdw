      // Summary: Cancels the current selection
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure CancelSelection()
      
      
      // Cancels the selection
      FOR EACH ROW i of LOOP_FoldersFiles
      	LOOP_FoldersFiles[LOOP_FoldersFiles]..BackgroundColor	= Transparent
      	LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_Selection		= False
      end
      
      // Reset the information
      gbSelectionMode = False
      ManageAvailOption()
      
