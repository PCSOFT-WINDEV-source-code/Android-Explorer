      // Summary: Manages the available options according to the selected elements
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure ManageAvailOption()
      
      // retrieves the selection
      bSelectionStatus is boolean = CheckSelection()
      
      // Updates the visibility of options
      OPT_Copy.Visible	= bSelectionStatus
      OPT_Cut.Visible		= bSelectionStatus
      OPT_Delete.Visible	= bSelectionStatus
      OPT_Zip.Visible		= bSelectionStatus
      
      
      // A single element can be renamed, adjust the visibility of the option according to the current selection 
      sCurrentSelection is string = GetSelection()
      if sCurrentSelection = "" or Contains(sCurrentSelection,CR) then
      	OPT_Rename.Visible = False
      else	
      	OPT_Rename.Visible = True
      end
      
      // manage the copy/cut/paste mode
      if gnCopyCutMode = NoOperation then
      	OPT_Paste.Visible= False
      else
      	OPT_Paste.Visible= True
      end
      
