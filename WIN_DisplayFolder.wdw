     name : ManageFolderSelection
     procedure_id : 1409354674524364010
     type_code : 14
     code : |1+
      // Summary: Manages the selection or the start of an associated application
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure ManageFolderSelection()
      
      // error case
      if LOOP_FoldersFiles <= 0 or LOOP_FoldersFiles > LOOP_FoldersFiles.Count then
      	return
      end
      
      // In selection mode, add an element to the selection
      if gbSelectionMode = True then
      	// Color the selected element
      	LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_Selection = not(LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_Selection)
      	if LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_Selection = False then
      		// Delete the selection from the selection
      		LOOP_FoldersFiles[LOOP_FoldersFiles]..BackgroundColor	= Transparent
      		
      		// if no selection
      		gbSelectionMode											= CheckSelection()
      		
      	else
      		// Select the additional element
      		LOOP_FoldersFiles[LOOP_FoldersFiles]..BackgroundColor = DarkGray
      	end
      else
      	
      	// Re-open the window with different parameters (used to get the activity return via the return button without having to code anything)
      	gsFileList = ""
      	ListType(geCurrentExtensionType,False,LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_FullPath)
      	
      	while gsFileList = ""
      		Multitask(-10)
      	end
      	
      	// Empty 
      	if gsFileList = "__-1__" then
      		Info(<ยง$0001ยง>)
      		return
      	end
      	
      	OpenMobileWindow(WIN_DisplayFolder,gsFileList,LOOP_FoldersFiles[LOOP_FoldersFiles].STC_Name,geCurrentExtensionType,False,LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_FullPath,"ManageAvailOption")
      	
      end
     type : 458752
   -
     name : DisplayContent
     procedure_id : 1409354674524429546
     type_code : 14
     code : |1-
      // Summary: Displays the content of gsListToDisplay variable in the window
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure DisplayContent()
      
      // Clear the area before displaying the content 
      LooperDeleteAll(LOOP_FoldersFiles)
      
      if gbDirMode then
      	////////////////////////
      	// DIRECTORY MODE
      	///////////////////////
      	
      	
      	// Array containing all files/folders with the information for their displays
      	aaFolder is ASSOCIATIVE ARRAY of int
      	
      	// Get the folders containing the files
      	for each string sFile of gsListToDisplay SEPAREE by CR
      		if aaFolder[fExtractPath(sFile,fDrive+fDirectory)] = 0 then
      			// New folder
      			aaFolder[fExtractPath(sFile,fDrive+fDirectory)] = 1
      		else
      			// Existing folder, add the fact that a file is in this folder as well
      			aaFolder[fExtractPath(sFile,fDrive+fDirectory)]+=1
      		end
      	end
      	
      	// Add the folders
      	for each nValue,sKey of aaFolder
      		
      		// Error case
      		if sKey ~= "" then
      			continue
      		end
      		
      		// Check the search
      		if ACTB_ActionBar.SearchValue <> "" _and_ Contains(sKey,ACTB_ActionBar.SearchValue,IgnoreCase) = False then
      			// goes to next element
      			continue
      		end	
      		
      		// Get the name of the folder containing the files
      		nAdditionSubscript is int = LooperAddLine(LOOP_FoldersFiles)
      		
      		if sKey+[fSep] = SysDirStorageCard()+[fSep] then
      			LOOP_FoldersFiles[nAdditionSubscript].STC_Name = "Internal storage memory"
      		else
      			LOOP_FoldersFiles[nAdditionSubscript].STC_Name = ExtractString(sKey,2,fSep,FromEnd) + " (" + nValue + ")"
      		end
      		
      		// Fill the information
      		LOOP_FoldersFiles[nAdditionSubscript].ATT_FullPath	= sKey
      		LOOP_FoldersFiles[nAdditionSubscript].ATT_Selection	= False
      		LOOP_FoldersFiles[nAdditionSubscript].ATT_Type		= type_Folder
      	end
      	
      else
      	
      	////////////////////////
      	// FILE + DIRECTORY MODE
      	///////////////////////
      	
      	for each string sFile of gsListToDisplay SEPAREE by CR
      		
      		// Error case
      		if sFile ~= "" then
      			continue
      		end
      		
      		// Check the search
      		if ACTB_ActionBar.SearchValue <> "" _and_ Contains(sFile,ACTB_ActionBar.SearchValue,IgnoreCase) = False then
      			// goes to next element
      			continue
      		end	
      		
      		// Addition subscript
      		nAdditionSubscript is int
      		
      		// Folder or file?
      		if fDirExist(sFile) = True then
      			// Folder
      			nAdditionSubscript = LooperAddLine(LOOP_FoldersFiles)
      			
      			
      			// special case of internal memory
      			if sFile = SysDirStorageCard then
      				LOOP_FoldersFiles[nAdditionSubscript].STC_Name = "Internal storage memory"
      			else
      				LOOP_FoldersFiles[nAdditionSubscript].STC_Name = ExtractString(sFile,1,fSep,FromEnd)
      			end
      			
      			// Fill the information
      			LOOP_FoldersFiles[nAdditionSubscript].ATT_FullPath	= sFile
      			LOOP_FoldersFiles[nAdditionSubscript].ATT_Selection	= False
      			LOOP_FoldersFiles[nAdditionSubscript].ATT_Type		= type_Folder
      		else
      			
      			if fFileExist(sFile) = True
      				// File
      				nAdditionSubscript									= LooperAddLine(LOOP_FoldersFiles)
      				
      				// Fill the information
      				LOOP_FoldersFiles[nAdditionSubscript].STC_Name		= ExtractString(sFile,1,fSep,FromEnd)
      				LOOP_FoldersFiles[nAdditionSubscript].ATT_FullPath	= sFile
      				LOOP_FoldersFiles[nAdditionSubscript].ATT_Type		= type_File
      				LOOP_FoldersFiles[nAdditionSubscript].IMG_Logo		= getImageAccordingToExtension(sFile)
      				LOOP_FoldersFiles[nAdditionSubscript].ATT_Selection	= False
      			end
      		end
      		
      	end
      end
      
      // Manages the options displayed in the action bar
      ManageAvailOption()
     type : 458752
