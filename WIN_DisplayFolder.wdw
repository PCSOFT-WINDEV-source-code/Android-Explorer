     code : |1+
      // Summary: Retrieves the elements selected in the window
      // Parameters:
      // Return value:
      // 	UNICODE string: list of selected elements
      //
      procedure GetSelection() : string
      
      // Get the selection 
      sSelectionString is string = ""
      
      // For all the elements possibly selected (the looper)
      FOR EACH ROW nSubscript of LOOP_FoldersFiles
      	
      	// If the element is selected, add its full path to the list
      	if LOOP_FoldersFiles[nSubscript].ATT_Selection = True then		
      		sSelectionString+= [CR] + LOOP_FoldersFiles[nSubscript].ATT_FullPath
      	end
      end
      
      // Returns the selection
      return sSelectionString
      
     type : 458752
