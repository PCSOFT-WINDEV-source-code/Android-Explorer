      // Summary: Initializes the window 
      // Parameters:
      //	sNewFile (UNICODE string - default value=""): New file to add if necessary
      // Return value:
      // 	None
      //
      procedure WindowInitialisation(local sNewFile is string = "")
      
      // Retrieves the elements possibly added by the previous windows
      gsListToDisplay+=[CR]+sNewFile
      
      // Displays the looper
      LOOP_FoldersFiles.Visible = True
      
      // Displays the content
      DisplayContent()
      
      // Manage the visibility of 'new folder' option
      OPT_New_folder.Visible	= not gbDirMode
      
      // Manage the selection and the available options
      gbSelectionMode			= False
      ManageAvailOption
      
      // Deletes the global selection
      LooperSelectMinus(LOOP_FoldersFiles)
      
     type : 458752
