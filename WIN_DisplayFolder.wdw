   -
     name : OPT_Paste
     identifier : 0x238f089c1bd79cf9
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          // Disable the option
          OPT_Paste.Visible = False
          
          // Perform the proper action according to the selected option
          if gnCopyCutMode = Copy then
          	if PerformCopy = False then
          		return
          	end
          end
          
          if gnCopyCutMode = Cut then
          	// A cut is in fact a copy/paste then a delete
          	
          	// Copy 
          	if PerformCopy() = False then
          		return
          	end
          	
          	// Delete
          	for each string sFile of gsCopyCutFile
          		
          		// Deleting elements
          		ElementDeletion(sFile)
          	end
          end
          
          // Reset the window
          WindowInitialisation(gsCopyCutFile)
          
          // Reset the information of copy/paste mode
          gsCopyCutFile	= ""
          gnCopyCutMode	= NoOperation
          
          
          	INTERNAL PROCEDURE PerformCopy
          		// Browse all files to perform the copy
          		for each string sFile of gsCopyCutFile
          			
          			// Error case
          			if sFile ~="" then
          				continue
          			end
          			
          			// Copy the directories
          			if fDirExist(sFile) then
          				// Create the directory
          				fMakeDir(gsCurrentDir+[fSep]+ExtractString(fExtractPath(sFile,fDirectory),1,fSep,FromEnd))
          				
          				// Perform the copy manually
          				if fCopyFile(sFile+[fSep]+"*.*",gsCurrentDir+[fSep]+sFile,frRecursive) = False then
          					Error(ErrorInfo(errFullDetails))
          					return False
          				end
          			end
          			
          			// Copying files
          			if fFileExist(sFile) then
          				if fCopyFile(sFile,gsCurrentDir+[fSep]) = False then
          					Error(ErrorInfo(errFullDetails))
          					return False
          				end
          			end
          		end
          		return True
          	end
         type : 32
