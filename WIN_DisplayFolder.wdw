      // Summary: Deletes an element
      // Parameters:
      //	sFullPath (UNICODE string): Element path
      //	sType (UNICODE string - default value=""): Type of file
      // Return value:
      // 	None
      //
      procedure ElementDeletion(local sFullPath is string, local sType is string = "")
      
      
      // If unknown type, check whether it is a folder or a file
      if fDirExist(sFullPath) then
      	sType = type_Folder
      end
      if fFileExist(sFullPath) then
      	sType = type_File
      end
      
      // According to the type of current element
      switch sType
      
      	// Folder
      	case type_Folder
      		
      		// Deletes the folder recursively
      		if fRemoveDir(sFullPath,frRecursive)	 = False then
      			Error(ErrorInfo(errFullDetails))
      			return
      		end
      		
      		// Deletes the element fom the list to display
      		gsListToDisplay = 	Replace(gsListToDisplay,sFullPath,"")
      		
      	// File				
      	case type_File
      		
      		// Deletes the file
      		if fDelete(sFullPath) = False then
      			Error(ErrorInfo(errFullDetails))
      			return
      		end
      		
      		// Deletes the element fom the list to display
      		gsListToDisplay = 	Replace(gsListToDisplay,sFullPath,"")	
      		
      		
      	other case
      		// Error
      		Error("Unknown type")
      end
