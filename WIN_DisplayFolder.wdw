      // Summary: Manages the selection or the start of an associated application
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure ManageFileSelection()
      
      // error case
      if LOOP_FoldersFiles <= 0 or LOOP_FoldersFiles > LOOP_FoldersFiles.Count then
      	return
      end
      
      // In selection mode, add an element to the selection
      if gbSelectionMode = True then
      	// Color the selected element
      	LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_Selection = not(LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_Selection)
      	
      	// If the element is already selected, then its attribute was just switched to false	
      	if LOOP_FoldersFiles[LOOP_FoldersFiles].ATT_Selection = False then
      		LOOP_FoldersFiles[LOOP_FoldersFiles]..BackgroundColor	= Transparent
      		
      		// if no selection
      		gbSelectionMode											= CheckSelection()
      		
      	else
      		// The element was just selected, change the background color
      		LOOP_FoldersFiles[LOOP_FoldersFiles]..BackgroundColor = DarkGray
      	end
      else
      	// Otherwise, start the application associated with the file
      	ShellExecute(LOOP_FoldersFiles.ATT_FullPath)
      end
