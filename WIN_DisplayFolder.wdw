      // Summary: Checks the current selection
      // Parameters:
      //	None
      // Return value:
      // 	boolean: True if a selection is in still progress
      //
      procedure CheckSelection() : boolean
      
      // No selection by default
      bSelection is boolean = False
      
      // browses the looper
      for i=1 _to_ LOOP_FoldersFiles.Count
      	
      	// A selection? 
      	if bSelection = True then return bSelection
      	
      	// Retrieves the looper attribute
      	bSelection = LOOP_FoldersFiles[i].ATT_Selection
      end
      
      // Selection? (useful for the last line
      if bSelection = True then return bSelection
      
      // no selection
      return False
