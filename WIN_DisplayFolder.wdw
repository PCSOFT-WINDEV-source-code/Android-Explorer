   -
     name : OPT_Zip
     identifier : 0x238f089c1bd39cf9
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         internal_properties : CAAAAAgAAACbXULZJfffll9sD2q8R6j5m60cbOhNAxM36nu4+NXPJ8HkVuDrCNl9+EMDBJCe9e5/CjpwEHTeEJKZHPGshHLQua1Qp8EgtJS4+lZAovbM1lfVpI/WD6ITRCSxCMM1i3ARMRONTjiuAvTGZzw=
         code : |1+
          // Prepare zip
          sArchiveName		is string
          sFullArchiveName	is string
          
          // Type the archive name
          if Input("Archive name?",sArchiveName) = 0 _or_ sArchiveName = "" then
          	// We don't want to create any archive
          	return
          end
          
          // Create the archive
          
          // Formats the archive name
          sFullArchiveName	= sArchiveName
          sArchiveName		= fExtractPath(sArchiveName,fFile)
          
          // Force the zip if no extension
          if fExtractPath(gsCurrentDir+[fSep]+sFullArchiveName,fExtension) ~= "" then
          	sFullArchiveName +=".zip"
          end
          
          // Creates the archive
          if zipCreate(sArchiveName,gsCurrentDir+[fSep]+sFullArchiveName) > 0  then
          	Error(ErrorInfo(errFullDetails))
          	return
          end
          
          // Add the selected elements into the archive
          FOR EACH ROW i of LOOP_FoldersFiles
          	
          	// Element not selected, goes to the next one
          	if LOOP_FoldersFiles[i].ATT_Selection = False then
          		continue
          	end
          	
          	
          	switch LOOP_FoldersFiles[i].ATT_Type
          	
          		// If it's a folder
          		case LOOP_FoldersFiles
          			if zipAddDirectory(sArchiveName,LOOP_FoldersFiles[i].ATT_FullPath) > 0 then
          				Error(ErrorInfo(errFullDetails))
          				return
          			end
          			
          		// If it's a file
          		case type_File 
          			if zipAddFile(sArchiveName,LOOP_FoldersFiles[i].ATT_FullPath,zipNone) > 0 then
          				Error(ErrorInfo(errFullDetails))
          				return
          			end
          			
          			
          		other case
          			// Error 
          			Error("Unknown file type")		
          	end
          end
          
          // Closes the archive
          zipClose(sArchiveName)
          
          // Message
          ToastDisplay("Archive created successfully")
          
          // Refreshes the window
          WindowInitialisation(gsCurrentDir+[fSep]+sFullArchiveName)
         type : 32
